<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:auto="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".TestCase.progressbar.view.ShowWEBActivity">

    <com.myself.rxjavasamsples.library.view.NavigationBar
        style="@style/style_navigation_bar"
        auto:nav_divider_color="@color/divider_color"
        auto:nav_has_divider="true"
        auto:nav_left_icon="@drawable/back_selector"
        auto:nav_main_title="展示Webivew"/>

    <!--加载进度条-->
    <ProgressBar
        android:id="@+id/pb_progress"
        style="@android:style/Widget.ProgressBar.Horizontal"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:progressBackgroundTint="#00000000"
        android:progressTint="@color/text_main_color_sel"/>

    <com.myself.rxjavasamsples.TestCase.progressbar.view.ProgressWebView
        android:id="@id/progressBar"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

</LinearLayout>
